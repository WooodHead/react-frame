.container
  position: absolute
  top: 0
  left: 0
  width: 100%
  height: 60px
  background-color: #fff
  box-shadow: 0 0 5px #888
  border-bottom: 1px solid #888\9
  z-index: 1041
  .logo
    position: absolute
    left: 0
    top: 0
    width: 110px
    height: 100%
    background: url('@/imgs/logo1.png') center center no-repeat
  .version
    font-size: 12px
    color: #fff
    position: absolute
    left: 120px
    top: 28px
    padding: 0 10px
    height: 20px
    line-height: 20px
    text-align: center
    border-radius: 4px
    background-color: #ff5608
