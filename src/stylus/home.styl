.goback
  width: 108px
  height: 90px
  position: absolute
  top: 0
  left: 0
  background-image: url('@/imgs/navbar_back.png'), -webkit-linear-gradient(top, #005ccc, #0083ff)
  background-repeat: no-repeat, no-repeat
  background-size: 40px 40px, 100%
  background-position: 30px center , center
.navbar-right
  background:  -webkit-linear-gradient(top, #005ccc, #0083ff)
  width: 108px
  height: 90px
  position: absolute
  top: 0
  right: 0
  display: flex
  align-items: center
  justify-content: flex-end
.home-tabs
  background: transparent
  height: 100%
  display: flex
  flex-direction: column
  &:global > .am-tabs-bar
    height: 90px !important
    overflow: hidden
    margin: 0 108px !important
    background: -webkit-linear-gradient(top, #005ccc, #0083ff) !important
    .am-tabs-prevpage,.am-tabs-nextpage
      &:before
        background: transparent
      &:after
        background: transparent
    .am-tabs-ink-bar
      background: #FFFFFF
      height: 6px
    .am-tabs-tab
      opacity: 0.7;
      font-family: PingFangSC-Regular;
      font-size: 28px;
      color: #FFFFFF;
      letter-spacing: 0.12px;
      background: transparent !important;
      border: none
      line-height: 90px
      height: 90px
    .am-tabs-tab-active
      opacity: 1;
      font-family: PingFangSC-Medium;
      font-size: 32px;
      color: #FFFFFF;
      letter-spacing: 0.13px;
  :global
    .am-tabs-content-no-animate
      overflow: scroll
    .am-tabs-content
      flex: 1
      overflow: scroll
